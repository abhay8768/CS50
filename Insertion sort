
/insertion sort
#include <stdio.h>

int main(void)
{
    int s[] = {8, 5, 3, 2, 1, 4, 6, 7};
    int i, j;
    int n = sizeof(s)/sizeof(s[0]);
    for(i = 1; i < n; i++)
    {
        int key = s[i];
        j = i - 1;
        while(j >= 0 && s[j] > key)
        {
            s[j+1] = s[j];
            j = j - 1;
        }
        s[j+1] = key;
    }
    for(i = 0; i < n; i++)
    {
        printf("%d ", s[i]);
    }
    
}
